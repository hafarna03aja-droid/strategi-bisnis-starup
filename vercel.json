{
  "name": "ai-perencana-strategi-startup",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  },
  "build": {
    "env": {
      "GEMINI_API_KEY": "@gemini_api_key"
    }
  }
}